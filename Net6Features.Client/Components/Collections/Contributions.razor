@inherits CollectionComponentBase<Net6Features.Shared.Models.Contribution>

<HeadContent>
    <meta name="description" content="Collection for TT Contributions" />
</HeadContent>

<DataTable TItem="@Net6Features.Shared.Models.Contribution"
           Title="Contributions"
           Row="Title"
           Data="_data"
           IsLoading="_loadingData"
           SearchTerm="@SearchTerm"
           UseGrpcChanged="ReloadDataAsync"
           ValueExpression="((d) => d.Title)"
           SearchExpression="@((d, s) => d.Title.Contains(s, StringComparison.CurrentCultureIgnoreCase))" />


