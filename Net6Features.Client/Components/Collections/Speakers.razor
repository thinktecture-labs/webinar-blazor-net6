@inherits CollectionComponentBase<Net6Features.Shared.Models.Speaker>

<HeadContent>
    <meta name="description" content="Collection for TT Speakers" />
</HeadContent>

<DataTable TItem="@Net6Features.Shared.Models.Speaker"
           Title="Contributions"
           Row="Title"
           Data="_data"
           IsLoading="_loadingData"
           SearchTerm="@SearchTerm"
           UseGrpcChanged="ReloadDataAsync"
           ValueExpression="@((d) => $"{d.FirstName} {d.LastName}")"
           SearchExpression="@((d, s) => $"{d.FirstName} {d.LastName}".Contains(s, StringComparison.CurrentCultureIgnoreCase))" />